<% if flash[:notice] %>
	<%= js add_gritter(flash[:notice], title: "Over Time Notification", sticky: false, time: 2000) %>
<% elsif flash[:alert] %>
	<%= js add_gritter(flash[:alert], title: "Over Time Notification", sticky: false, time: 2000) %>
<% elsif flash[:error] %>
	<%= js add_gritter(flash[:error], title: "Over Time Notification", sticky: false, time: 2000) %>
<% end %>